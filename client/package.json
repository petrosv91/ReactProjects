{
  "name": "phytothreptiki_client",
  "productName": "Φυτοθρεπτική App",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "public/electron.js",
  "dependencies": {
    "@chakra-ui/react": "^1.1.5",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@hookform/resolvers": "^1.0.1",
    "@xstate/react": "^1.0.3",
    "axios": "^0.21.1",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^1.2.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^2.9.4",
    "react": "^16.14.0",
    "react-datepicker": "^3.4.1",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.14.2",
    "react-icons": "^3.11.0",
    "react-query": "^2.25.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.4",
    "react-to-print": "^2.12.0",
    "uuid": "^8.3.1",
    "xstate": "^4.16.0",
    "yup": "^0.31.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-build": "yarn build && yarn electron-builder",
    "electron-start": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "author": "Petros Vasilopoulos",
  "license": "ISC",
  "eslintConfig": {
    "extends": "react-app"
  },
  "build": {
    "appId": "phytothreptiki-app",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/logos/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "public/logos/favicon.ico",
      "uninstallerIcon": "public/logos/favicon.ico",
      "uninstallDisplayName": "Φυτοθρεπτική App",
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "wait-on": "^5.2.1",
    "electron": "^11.3.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.0.0",
    "electron-builder": "^22.9.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
